//C1
#include<bits/stdc++.h>
using namespace std;
#define ll long long

int main(){
	ll t;
	cin>>t;
	for(ll i=0;i<t;i++){
		ll n,k;
		cin>>n>>k;
		vector<ll> v;
		for(ll j=0;j<n;j++){
			ll la;
			cin>>la;
			v.push_back(la);
		}
		ll start=0;
		ll count=0;
		for(ll j=0;j<n;j++){
			if(start==1 && v[j]==v[j-1]-1){
				start=1;
				if(v[j]==1){
					count=count+1;
				}
			}
			else{
				start=0;
			}

			if(v[j]==k){
				start=1;
			}

		}
		cout<<"Case #"<<i+1<<":"<<" "<<count<<endl;
	}
	return 0;
}


//D1
#include<bits/stdc++.h>
using namespace std;
#define ll long long

int main(){
	ll t;
	ll p;
	cin>>t;
	p=t;
	while(t--){
		ll n;
		cin>>n;
		vector<ll> a;
		for(ll i=0;i<n;i++){
			ll la;
			cin>>la;
			a.push_back(la);
		}
		ll count=0;
		ll maxa = 0;
		for(ll i=0;i<n;i++){
			if((a[i]>maxa || i==0) && (a[i]>a[i+1] || i==n-1)){
				count++;
			}
			maxa = max(maxa,a[i]);			
		}
		cout<<"Case #"<<p-t<<":"<<" "<<count<<endl;
	}
	return 0;
}
//D2
#include<bits/stdc++.h>
using namespace std;
#define ll long long

int main(){
	ll t;
	ll p;
	cin>>t;
	p=t;
	while(t--){
		ll n;
		cin>>n;
		vector<ll> v;
		for(ll i=0;i<n;i++){
			ll la;
			cin>>la;
			v.push_back(la);
		}
		ll count4 = 1;
		ll count=0;
		for(ll i=1;i<n;i++){
			
			if(v[i]>v[i-1]){
				count4++;
				if(count4>4){
					count++;
					count4=1;
				}
			}
			else if(v[i]<v[i-1]){
				count4=1;
			}
		}
		count4=1;
		for(ll i=1;i<n;i++){
			
			if(v[i]<v[i-1]){
				count4++;
				if(count4>4){
					count++;
					count4=1;
				}
			}
			else if(v[i]>v[i-1]){
				count4=1;
			}
		}
		
		cout<<"Case #"<<p-t<<":"<<" "<<count<<endl;
	}
	return 0;
}
//D3
#include<bits/stdc++.h>
using namespace std;
#define ll long long

int main(){
	ll t;
	ll p;
	cin>>t;
	p=t;
	while(t--){
		ll n,q;
		cin>>n>>q;
		n = n-1;
		vector<ll> v;
		for(ll i=0;i<n;i++){
			ll la;
			cin>>la;
			v.push_back(la);
		}
		vector<ll> ret;
		while(q--){
			ll s, k;
			cin>>s>>k;
			k=k-1;
			ll i,j;
			i=s-2;
			j=s-1;
			
			ll flag=0;
			while(k--){
				if(j>n-1){
					i--;
					flag=1;
				}
				else if(i<0){
					j++;
					flag=2;
				}
				else if(v[i]<v[j]){
					i--;
					flag=1;
				}
				else if(v[j]<v[i]){
					j++;
					flag=2;
				}
				// cout<<"flag:"<<flag<<" ";
			}
			// cout<<endl;
			if(flag==1){
				ret.push_back(i+2);
			}
			else if(flag==2){
				ret.push_back(j+1);
			}
			else{
				ret.push_back(j+1);
			}

		}
		cout<<"Case #"<<p-t<<": ";
		for(ll i=0;i<ret.size();i++){
			cout<<ret[i]<<" ";
		}
		cout<<endl;
	}
	return 0;
}
